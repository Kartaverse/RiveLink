end


function Process(req)
	local img = InImage:GetValue(req)

	local currentFrame = req.Time
	local date = os.date("%Y-%m-%d")

	-- Get the current node name
	local nodeName = self.Name

	-- Should detailed logging be printed to the Console
	local verbose = InVerbose:GetValue(req).Value

	-- Get the rive document name
	local rive_name = InRive:GetValue(req).Value

	if req:IsPreCalc() then
		if verbose == 1 or verbose == true then
			print("[RiveLink] [Precalc Pass]")
		end

		OutImage:Set(req, img)
		return
	end

	-- Interactive Rendering enabled
	local interactive = InInteractive:GetValue(req).Value
	if (interactive == 1 and req:IsInteractive()) or not req:IsInteractive() then
		-- Check for a non nil string
		if rive_name and rive_name ~= "" then
			if jit.os == "OSX" then
				-- tbd
			else
				print("[RiveLink] [Warning] This fuse only runs on macOS!")
			end
		else
			print("[RiveLink] [Warning] The Rive File field is empty!")
		end
	else
		if verbose == 1 or verbose == true then
			print("[RiveLink] [Interactive Rendering] Disabled")
		end
	end

	OutImage:Set(req, img)
end
